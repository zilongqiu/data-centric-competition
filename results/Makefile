
.PHONY: new_version
new_version:
	mkdir $(VERSION)
	mkdir $(VERSION)/model
	mkdir $(VERSION)/train
	mkdir $(VERSION)/train/i
	mkdir $(VERSION)/train/ii
	mkdir $(VERSION)/train/iii
	mkdir $(VERSION)/train/iv
	mkdir $(VERSION)/train/v
	mkdir $(VERSION)/train/vi
	mkdir $(VERSION)/train/vii
	mkdir $(VERSION)/train/viii
	mkdir $(VERSION)/train/ix
	mkdir $(VERSION)/train/x
	mkdir $(VERSION)/val
	mkdir $(VERSION)/val/i
	mkdir $(VERSION)/val/ii
	mkdir $(VERSION)/val/iii
	mkdir $(VERSION)/val/iv
	mkdir $(VERSION)/val/v
	mkdir $(VERSION)/val/vi
	mkdir $(VERSION)/val/vii
	mkdir $(VERSION)/val/viii
	mkdir $(VERSION)/val/ix
	mkdir $(VERSION)/val/x
